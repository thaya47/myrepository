ch=input("enter the character:")
if(ch=='a' or ch=='e' or ch=='i' or ch=='o' or ch=='u' or ch=='A' or ch=='E' or ch=='I' or ch=='O' or ch=='U'):
    print("your charactert is vowel")
elif(ch='b' or ch='c' or ch='d' or ch='f' or ch='g' or ch='j' or ch='k' or ch='l' or ch='m' or ch='n' or ch='p' or ch='q' or ch='r' or ch='s' or ch='t' or ch='v' or ch='w' or ch='x' or ch='y' or ch='z' or ch='B' or ch='C' or ch='D' or ch='F' or ch='G' or ch='H' or ch='J' or ch='K' or ch='L' or ch='M' or ch='N' or x='P'x='Q' or x='R' or x='S'x='T' or x='U' or x='V'or x='W' or x='X' or x='Y' or x='Z'):
    print("your charactert is consonant")
else:
    print("it is not a character")
